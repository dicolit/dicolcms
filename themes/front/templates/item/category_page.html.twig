{% extends '@front/page.html.twig' %}
{% block title %}{{ page_title }}{% endblock %}
{% block stylesheets %}{% endblock %}
{% block content %}
    <div class="row">
        <div class="col-lg-12">
            <h1>Category List</h1>
            <ul>
            {% for item in item_list %}
                <li>
                    <a href="{{ item.href }}">
                        {{ item.title }}
                    </a>
                </li>
            {% endfor %}
            </ul>
        </div>
    </div>

    <hr class="featurette-divider">

    <!-- /END THE FEATURETTES -->
{% endblock %}
{% block javascripts %}{% endblock %}